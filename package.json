{
  "name": "sigilzero",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "prebuild": "rm -rf docs",
    "build": "next build && cp -r out docs",
    "start": "next start",
    "lint": "next lint",
    "check-frontmatter": "node ./scripts/check-frontmatter.js",
    "prepare": "husky",
    "validate:content": "npm run check-frontmatter && node ./scripts/validate-mixtapes.mjs && node ./scripts/validate-releases.mjs && node ./scripts/validate-artists.mjs && node ./scripts/validate-series.mjs",
    "validate:quick": "npm run check-frontmatter && node ./scripts/validate-mixtapes.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "ci:test": "npm run check-frontmatter && vitest run && next build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@next/mdx": "^16.0.7",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rehype": "^13.0.2",
    "rehype-raw": "^7.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
